<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_pressed="true">
        <bitmap android:src="@drawable/checkbox_check_press_background"/>
    </item>
    <item android:state_checked="true" android:state_enabled="false">
        <bitmap android:src="@drawable/checkbox_check_disable_background"/>
    </item>
    <item android:state_checked="true">
        <bitmap android:src="@drawable/checkbox_check_background"/>
    </item>
    <item android:state_checked="false" android:state_pressed="true">
        <bitmap android:src="@drawable/checkbox_uncheck_press_background"/>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <bitmap android:src="@drawable/checkbox_uncheck_disable_background"/>
    </item>
    <item>
        <bitmap android:src="@drawable/checkbox_uncheck_background"/>
    </item>
</selector>